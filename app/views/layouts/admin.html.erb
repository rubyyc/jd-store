<!DOCTYPE html>
<html>
<head>
  <title>Job Listing </title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>

<div class="container-fluid container-min-height">
  <div class="row">
    <%= render "common/navbar" %>
  </div>

  <%= render "common/flashes" %>
  <div class="row ">
    <div class="col-md-1">
      <ul class="nav nav-pills nav-stacked" style="max-width: 300px; padding-top:20px;">
        <li class="active"><%= link_to("Products", admin_products_path ) %> </li>

        <li class="active"><%= link_to("Members", admin_products_path ) %> </li>

      </ul>
    </div>

    <div class="col-md-10">
      <%= yield %>
    </div>
  </div>

</div>

<%= render "common/footer" %>

<script src="/layer-v3.1.1/layer/layer.js"></script>
<script src="/layer-v3.1.1/layer/dialog.js"></script>

</body>
</html>